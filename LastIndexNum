import java.util.*;
class LastIndex {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int m=in.nextInt();
        int a[]=new int[m];
        for(int i=0;i<a.length;i++){
           a[i]=in.nextInt();
        }
        int n=in.nextInt();
        for(int i=a.length-1;i>0;i--){
            if(n == a[i]){
                System.out.println(i+" ");
                break;
            }
        }
    }
}
